<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>

      <!-- 1. Bỏ WebDAV module -->
      <modules>
        <!-- Loại bỏ WebDAVModule kế thừa từ mức server / site -->
        <remove name="WebDAVModule" />
      </modules>

      <!-- 2. Bỏ WebDAV handler, chỉ giữ lại aspNetCore -->
      <handlers>
        <!-- Xóa handler WebDAV nếu đang được kế thừa -->
        <remove name="WebDAV" />

        <!-- Handler của ASP.NET Core -->
        <add name="aspNetCore"
             path="*"
             verb="*"
             modules="AspNetCoreModuleV2"
             resourceType="Unspecified" />
      </handlers>

      <aspNetCore processPath=".\BestProductManager.exe"
                  stdoutLogEnabled="false"
                  stdoutLogFile=".\logs\stdout"
                  hostingModel="inprocess" />
    </system.webServer>
  </location>
</configuration>

